    border-bottom-color: rgba(176, 120, 40, 0.95);
    border-left-color: rgba(255, 227, 141, 0.98);
}

.hud-inventory-slot-equipped .hud-inventory-slot-glow {
    opacity: 0.78;
}

.hud-inventory-slot-glow {
    position: absolute;
    left: -8px;
    right: -8px;
    top: -8px;
    bottom: -8px;
